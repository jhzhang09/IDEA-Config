<application>
  <component name="CodeInsightSettings">
    <option name="selectAutopopupSuggestionsByChars" value="true" />
    <option name="SHOW_PARAMETER_NAME_HINTS_ON_COMPLETION" value="true" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="500" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="SELECT_AUTOPOPUP_SUGGESTIONS_BY_CHARS" value="true" />
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
    <option name="JSP_ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DartCodeFoldingSettings">
    <option name="collapseParts" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="19" />
    <option name="FONT_FAMILY" value="MesloLGSDZ Nerd Font" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="MesloLGLDZ Nerd Font" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="PREVIEW" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
  </component>
  <component name="SqlCodeCompletionSettings" AUTO_ALIAS_TABLE="true">
    <option name="autoAliasTable" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <filter name="TODO">
      <pattern index="0" />
    </filter>
    <filter name="FIXME">
      <pattern index="1" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>